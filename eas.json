{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "key": "v1-node-cache",
        "paths": ["node_modules"]
      }
    }
  },
  "submit": {
    "production": {}
  }
}


